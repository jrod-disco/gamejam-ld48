    "color": "^3.1.3",
    "firebase": "^7.18.0",
    "firebaseui": "^4.6.1"
  },
  "scripts": {
    "dev": "rollup -c -w --environment BUILD:dev",
    "dev:docker": "docker-compose up --build",
    "clean": "rimraf dist && rimraf dist-nwjs",
    "prod": "rimraf dist && rollup -c --environment BUILD:clean",
    "build": "rimraf dist && rollup -c --environment BUILD:prod",
    "docs": "rimraf docs && npx typedoc --excludeExternals --entryPoint 'main' --stripInternal --readme none",
    "nwjs-start": "run --mac-x64 --mirror https://dl.nwjs.io/ ./dist",
    "nwjs-dist": "rimraf dist-nwjs && build --tasks mac-x64 --mirror https://dl.nwjs.io/ ./dist",
    "all-the-things": "yarn build && yarn nwjs-dist"
  }
